{
  "expo": {
    "name": "BANSHI",
    "slug": "banshi-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "scheme": "banshifrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.banshi.app",
      "jsEngine": "hermes"
    },
    "android": {
      "package": "com.banshi.app",
      "jsEngine": "hermes",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon.png",
        "backgroundColor": "#FF6B35"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "a6539dae-f36d-48ac-b193-8b9d58e04f8c"
      }
    }
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/icons/icon.png"
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/icons/icon.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#FF6B35"
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "a6539dae-f36d-48ac-b193-8b9d58e04f8c"
    }
  },
  "owner": "kidfreeze"
}
